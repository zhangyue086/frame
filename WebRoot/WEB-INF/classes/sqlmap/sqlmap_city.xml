<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="city">
	<resultMap type="com.zline.zlogistics.biz.dal.entity.City" id="cityResult" >
		<id property="cityId" column="cityId"/>
		<result property="cityName" column="cityName"/>
		<result property="cityCode" column="cityCode"/>
	</resultMap>
	
	<resultMap type="com.zline.zlogistics.biz.dal.entity.City" id="keyResult" >
		<id property="cityId" column="city_id"/>
		<result property="cityName" column="city_name"/>
		<result property="cityCode" column="city_code"/>
	</resultMap>
	
	<select id="queryList" parameterType="com.zline.zlogistics.biz.dal.entity.DistributionStation" 
		resultMap="cityResult">
		select 
			city_id as cityId,
			city_code as cityCode,
			city_name as cityName
		from zlogistics.city
		where is_deleted = 0
	</select>
	<select id="queryByKey" parameterType="java.lang.Integer" 
		resultMap="keyResult">
		select 
			city_id ,
			city_code ,
			city_name 
		from zlogistics.city
		where is_deleted = 0
			AND  city_id =#{cityId}
	</select>
	
</mapper>