<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<!--<bean id="changeOrderSendMethod"
		class="com.zline.zlogistics.web.timer.ChangeOrderSendMethodTimer"
		autowire="byName">
	</bean>
	<bean id="changeSendMethod"
		class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">
		<property name="targetObject">
			<ref bean="changeOrderSendMethod" />
		</property>
		<property name="concurrent">
			<value>false</value>
		</property>
		<property name="targetMethod">
			<value>changeSendMethod</value>
		</property>
	</bean>
	<bean id="saveChangeSendMethod"
		class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">
		<property name="jobDetail">
			<ref bean="changeSendMethod" />
		</property>
		<property name="cronExpression">
			<value>40 0/5 * * * ?</value>
		</property>
	</bean>
	<bean class="org.springframework.scheduling.quartz.SchedulerFactoryBean">
		<property name="triggers">
			<list>
				<ref local="saveChangeSendMethod" />
			</list>
		</property>
	</bean>

--></beans>