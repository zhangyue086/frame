<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<!--读取配置项信息 -->
	<context:property-placeholder location="classpath:meta_config/*.properties" />

	<import resource="classpath:config/spring-datasource.xml" />
	<import resource="classpath:config/spring-dao.xml" />
	<import resource="classpath:config/spring-biz.xml" />
	<import resource="classpath:config/spring-action.xml" />
	<import resource="classpath:config/spring-timer.xml" />
	<!--
	<import resource="classpath:config/dubbo.xml" /> 
	-->
</beans>