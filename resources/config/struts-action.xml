<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="login" extends="struts-default">
		<action name="login" class="loginAction" method="userLogin">
			<result name="loginSuccess">/welcome.jsp</result>
			<result name="login">/html/global/login.jsp</result>
		</action>
		<action name="loginOut" class="loginAction" method="loginOut">
			<result name="login">/html/global/login.jsp</result>
		</action>
	</package>

	<package name="fatherPackage" extends="struts-default,json-default">
		<interceptors>
			<interceptor name="line0LoginInterceptor"
				class="com.zline.zlogistics.web.interceptor.LoginInterceptor" />
			<interceptor-stack name="mydefault">
				<interceptor-ref name="defaultStack" />
				<interceptor-ref name="line0LoginInterceptor" />
			</interceptor-stack>
		</interceptors>
		<default-interceptor-ref name="mydefault"></default-interceptor-ref>
		<global-results>
			<result name="login">/html/global/login.jsp</result>
			<result name="exception">/html/global/exception.jsp</result>
		</global-results>
	</package>
	<package name="dispatch" extends="fatherPackage" namespace="/dispatch">
		<action name="*_Dispatch" class="dispatchOrderAction" method="{1}">
			<interceptor-ref name="defaultStack"></interceptor-ref>
			<result name="manual">/html/dispatch/manual_dispatch.jsp</result>
			<result name="unresolverorder" type="json">
				<param name="root">logisticsOrderList</param>
				<param name="contentType">text/html</param>
			</result>
			<result name="distributionMember" type="json">
				<param name="root">distributionMemberList</param>
				<param name="contentType">text/html</param>
			</result>
			<result name="batchSetDM" type="json">
				<param name="root">message</param>
				<param name="contentType">text/html</param>
			</result>
		</action>
	</package>

	<package name="index" extends="fatherPackage">
		<action name="index" class="indexAction">
			<result name="index">/html/index.jsp</result>
		</action>
	</package>

