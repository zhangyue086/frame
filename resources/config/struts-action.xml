				<param name="contentType">text/html</param>
			</result>
			<result name="edit" type="json">
				<param name="root">message</param>
				<param name="contentType">text/html</param>
			</result>
			<result name="delete" type="json">
				<param name="root">message</param>
				<param name="contentType">text/html</param>
			</result>
		</action>
		
